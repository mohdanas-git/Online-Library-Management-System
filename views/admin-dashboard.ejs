<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Admin-Dasboard</title>
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/6.5.2/css/all.min.css">
    <link rel="stylesheet" href="style.css">
    <style>
        #book-det{
            position:relative;
            top: 60%;
        }
    </style>
</head>
<body>
    <%- include('admin-header') %>
    <p class="page-heading">Admin Dashboard</p>
    <div class="user-dash-area user-dash-area-u" id="book-area" style="background-color: rgb(08,19,39); position: relative; top: 150px; left: 30px;">
        <%- include('admin-sidemenu') %>
        <div class="inner-box">
            <a class="links book" href="/admin-listed-book" id="user-dashboard-back">
                <div id="margin-set">
                <i class="fa-solid fa-book" style="color: rgb(08,09,39); position: relative; left: 0px;"></i>
            <div id="book-det" style="position: relative; top: 10px;">
            <p class="link-num">9</p>
            <p class="links-para">Book Listed</p>
        </div>
            </div>
        </a>

        <a class="links book" href="/book-not-returned"
        id="user-dashboard-back" style="width: 60px;">
            <div id="margin-set">
                <i class="fa-solid fa-recycle" style="color: rgb(08,09,39); "></i>
                <div id="book-det">
                <p class="link-num">2</p>
            <p class="links-para">Not Returned Yet?</p>
            </div>
            </div>
        </a>

        <a class="links book" href="/registered-students"
        id="user-dashboard-back">
            <div id="margin-set">
                <i class="fa-solid fa-user-group" style="color: rgb(08,09,39);"></i>
                <div id="book-det">
                <p class="link-num">5</p>
            <p class="links-para">Registered Users</p>
            </div>
            </div>
        </a>

        <a class="links book" href="/author-listed"
        id="user-dashboard-back">
            <div id="margin-set">
                <i class="fa-solid fa-user" style="color: rgb(08,09,39);"></i>
                <div id="book-det">
                <p class="link-num">5</p>
            <p class="links-para">Author Listed</p>
            </div>
            </div>
        </a>

        <a class="links book" href="/admin-quizess"
        id="user-dashboard-back">
            <div id="margin-set">
                <i class="fa-solid fa-feather" style="color: rgb(08,09,39); position: relative; left: 10px;"></i>
                <div id="book-det" style="position: relative; top: 10px;">
                <p class="link-num">5</p>
            <p class="links-para">Quizess</p>
            </div>
            </div>
        </a>

        <a class="links book" href="/listed-categories"
        id="user-dashboard-back">
            <div id="margin-set">
                <i class="fa-solid fa-file-zipper" style="color: rgb(08,09,39);"></i>
                <div id="book-det">
                <p class="link-num">5</p>
            <p class="links-para">Listed Category</p>
            </div>
            </div>
        </a>

    </div>
    </div>
    <script src="script.js"></script>
    <%- include('footer') %>
</body>
</html>